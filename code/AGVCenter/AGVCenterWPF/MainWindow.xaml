                  RenderTransformOrigin="0.499,0.316">
                        <DataGrid.Columns>
                            <DataGridTextColumn Header="时间" Binding="{Binding Path=CreatedAtStr}"/>
                            <DataGridTextColumn Header="任务类型" Binding="{Binding Path=StockTaskTypeStr}"/>
                            <DataGridTextColumn Header="条码" Binding="{Binding Path=Barcode}"/>
                            <DataGridTextColumn Header="箱型" Binding="{Binding Path=BoxType}"/>
                            <DataGridTextColumn Header="巷道机" Binding="{Binding Path=RoadMachineIndex}"/>
                            <DataGridTextColumn Header="库位" Binding="{Binding Path=PositionNr}"/>
                            <DataGridTextColumn Header="层" Binding="{Binding Path=PositionFloor}"/>
                            <DataGridTextColumn Header="列" Binding="{Binding Path=PositionColumn}"/>
                            <DataGridTextColumn Header="排" Binding="{Binding Path=PositionRow}"/>
                            <DataGridTextColumn Header="AGV放行结果" Binding="{Binding Path=AgvPassFlag}"/>
                            <DataGridTextColumn Header="托量倒数" Binding="{Binding Path=TrayReverseNo}"/>
                            <DataGridTextColumn Header="托量" Binding="{Binding Path=TrayNum}"/>
                            <DataGridTextColumn Header="单量" Binding="{Binding Path=DeliveryItemNum}"/>
                            <DataGridTextColumn Header="状态" Binding="{Binding Path=StateStr}"/>
                            <DataGridTextColumn Header="数据库Id" Binding="{Binding Path=DbId}"/>
                        </DataGrid.Columns>
                    </DataGrid>
